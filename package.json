{
  "name": "leadership-legacy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "next build",
    "build:cloudflare": "CF_PAGES=1 next build",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "wrangler pages deploy out --project-name=leadership-legacy",
    "deploy:cloudflare": "./deploy-cloudflare.sh",
    "cf:d1:migrate": "wrangler d1 execute leadership-legacy-db --file=./cloudflare-schema.sql",
    "cf:d1:query": "wrangler d1 execute leadership-legacy-db --command",
    "cf:worker:tail": "wrangler tail",
    "cf:pages:tail": "wrangler pages deployment tail --project-name=leadership-legacy"
  },
  "dependencies": {
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.553.0",
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.22",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "^48.10.0"
  }
}
